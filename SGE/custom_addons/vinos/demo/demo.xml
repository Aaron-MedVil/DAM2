<odoo>
	<data>

		<!-- Modelo Variedad -->
		<record id="variedad1" model="variedad.vinos">
			<field name="name">Rojas</field>
		</record>

		<!-- Modelo Marcas -->
		<record id="marcas1" model="marca.vinos">
			<field name="name">Marca-ditto</field>
		</record>

		<!-- Modelo Denominacion Origen -->
		<record id="do1" model="do.vinos">
			<field name="name">Ribera del Duero</field>
		</record>

		<!-- Modelo Denominacion Origen -->
		<record id="bodega1" model="bodega.vinos">
			<field name="name">Bode-guita</field>
			<field name="location">Aranda de Duero</field>
			<field name="fundation">2014-09-01</field>
		</record>

		<!-- Modelo DO localizacion -->
		<record id="loDo1" model="doloc.vinos">
			<field name="do" ref="do1"/>
			<field name="location">Aranda de Duero</field>
		</record>
	</data>
</odoo>